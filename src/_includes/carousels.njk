<div class="owl-carousel owl-theme">
  {% for img in project.images %}
    <div class="item">
      <img src="{{ img }}" alt="Image for {{ project.name }}" loading="lazy">
    </div>
  {% endfor %}
</div>
<script>
  $(document).ready(function(){
    $(".owl-carousel").owlCarousel({
      items: 1,
      loop: true,
      autoplay: true,
      autoplayTimeout: 3000,
      nav: true,
      dots: true
    });
  });
</script>
